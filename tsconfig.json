{
  "transpiler": "typescript",
  "compilerOptions": {
    "outDir": "./lib/",
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "target": "ES2017",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  },
  "include": ["./src/**/*"],
  "exclude": ["./src/assets/**/*"]
}
